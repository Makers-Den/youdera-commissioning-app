{
  "name": "commissioning",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "npm run i18n && next build",
    "start": "next start",
    "test": "jest --coverage",
    "lint": "eslint src --fix",
    "extract:i18n": "formatjs extract \"src/{pages,components}/**/*.{js,ts,tsx}\" --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple content/locales content/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "next": "12.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.1",
    "tailwind-merge": "^1.6.2",
    "ui": "*"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.3",
    "@types/node": "^17.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.0",
    "babel-plugin-formatjs": "^10.3.31",
    "config": "*",
    "eslint": "^8.5.0",
    "eslint-plugin-formatjs": "^4.3.4",
    "next-transpile-modules": "^9.0.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.7",
    "tsconfig": "*",
    "typescript": "^4.5.4"
  }
}